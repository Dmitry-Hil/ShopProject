{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "paths": {
      "@Shared/*": [
        "./Shared/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}